{
   "name": "dnajs-smart-update-websockets",
   "description": "Example WebSockets server and client showing throttled live updates using dna.js",
   "version": "1.0.0",
   "license": "MIT",
   "type": "module",
   "main": "server.js",
   "repository": "github:dnajs/dnajs-smart-update-websockets",
   "homepage": "https://github.com/dnajs/dnajs-smart-update-websockets",
   "bugs": "https://github.com/dnajs/dnajs-smart-update-websockets/issues",
   "author": "Center Key (https://centerkey.com)",
   "keywords": [
      "node",
      "websockets",
      "dna.js",
      "javascript"
   ],
   "jshintConfig": {
      "esversion": 9,
      "strict": "implied",
      "eqeqeq": true,
      "undef": true,
      "unused": true,
      "browser": true,
      "node": true,
      "globals": {
         "dna": false,
         "WebSocket": true
      }
   },
   "scripts": {
      "test": "jshint . --exclude node_modules"
   },
   "dependencies": {
      "bufferutil": "~4.0",
      "express": "~4.17",
      "jshint": "~2.13",
      "utf-8-validate": "~5.0",
      "ws": "~8.4"
   },
   "devDependencies": {
      "@fortawesome/fontawesome-free": "~5.15",
      "dna.js": "~1.9",
      "jquery": "~3.6",
      "web-ignition": "~1.4"
   }
}
