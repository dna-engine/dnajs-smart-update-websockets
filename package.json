{
   "name": "dnajs-smart-update-websockets",
   "description": "Example WebSockets server and client showing throttled live updates using dna.js",
   "version": "1.0.0",
   "license": "MIT",
   "main": "server.js",
   "repository": "github:dnajs/dnajs-smart-update-websockets",
   "homepage": "https://github.com/dnajs/dnajs-smart-update-websockets",
   "bugs": "https://github.com/dnajs/dnajs-smart-update-websockets/issues",
   "author": "Center Key (https://centerkey.com)",
   "keywords": [
      "node",
      "websockets",
      "dna.js",
      "javascript"
   ],
   "jshintConfig": {
      "esversion": 9,
      "strict": "implied",
      "eqeqeq": true,
      "undef": true,
      "unused": true,
      "browser": true,
      "node": true,
      "globals": {
         "dna": false,
         "WebSocket": true
      }
   },
   "scripts": {
      "test": "jshint server.js web"
   },
   "dependencies": {
      "express": "~4.17",
      "jshint": "~2.10",
      "ws": "~7.0"
   },
   "devDependencies": {
      "@fortawesome/fontawesome-free": "~5.8",
      "dna.js": "~1.5",
      "jquery": "~3.4",
      "web-ignition": "~1.1"
   }
}
